{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modCobblemonVersion}-fabric-${modMyVersion}",
  "name": "${modName}",
  "description": "${modDescription}",
  "authors": [
    "${modAuthor}"
  ],
  "license": "MIT",
  "environment": "*",
  "depends": {
    "java": ">=21",
    "minecraft": "${minecraft_version}",
    "fabric": "*",
    "cobblemon": "${cobblemon_version}"
  },
  "contact": {
    "homepage": "${modDiscord}",
    "issues": "${modIssues}",
    "sources": "${modSource}"
  },
  "mixins": [
    "mixins.tim_core.json"
  ],
  "icon": "${modId}_small.png",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "us.timinc.mc.cobblemon.timcore.fabric.FabricTimCore"
      }
    ]
  }
}
